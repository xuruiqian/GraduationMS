<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernatebasicdemo.entities.StuCourseEntity" table="stu_course" schema="dbdemo">
        <composite-id mapped="true" class="com.hibernatebasicdemo.entities.StuCourseEntityPK">
            <key-property name="studentId" column="student_id"/>
            <key-property name="courseId" column="course_id"/>
        </composite-id>
    </class>
</hibernate-mapping>